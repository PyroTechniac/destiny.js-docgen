{
    "name": "destiny.js-docgen",
    "version": "0.0.1",
    "description": "The docs.json generator for destiny.js",
    "main": "stuff/index.js",
    "bin": {
        "docgen": "stuff/index.js"
    },
    "scripts": {
        "test": "eslint stuff && node stuff --source test/stuff --custom test/docs/index.yml --output test/docs/docs.json --spaces 2",
        "test:docs": "node stuff --source test/stuff --custom test/docs/index.yml --output test/docs/docs.json --spaces 2",
        "lint": "eslint stuff",
        "lint:fix": "eslint --fix stuff"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PyroTechniac/destiny.js-docgen.git"
    },
    "author": "Gryffon Bellish <gryffonbellish@gmail.com>",
    "dependencies": {
        "eslint": "^4.18.0",
        "js-yaml": "^3.10.0",
        "jsdoc-to-markdown": "^4.0.1",
        "tsubaki": "^1.3.0",
        "yargs": "^11.0.0"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "homepage": "https://github.com/PyroTechniac/destiny.js-docgen"
}
